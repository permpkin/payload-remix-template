{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:node": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts NODE_ENV=development nodemon ./main.ts --watch ./main.ts",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production bun ./server.ts",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build:p-payload": "yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types"
  },
  "dependencies": {
    "@remix-run/express": "^1.6.5",
    "@remix-run/node": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "payload": "^1.0.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.5",
    "@remix-run/eslint-config": "^1.6.5",
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.3",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
